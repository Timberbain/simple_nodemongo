{
  'extends': 'airbnb',
  'rules': {
    'import/no-dynamic-require': 0,
    'no-console': 0,
    'global-require': 0,
    'guard-for-in': 0,
    'no-param-reassign': 0,
    'no-loop-func': 0,
    'no-underscore-dangle': [
      'error',
      { 'allow': ['_id', '__v'] }
    ],
    'no-restricted-syntax': [
      'error',
      'LabeledStatement',
      'WithStatement',
    ],
    'no-use-before-define': [
      'error', 'nofunc'
    ],
    'consistent-return': 0,
    'no-eval': ['error', { 'allowIndirect': true }],
    'class-methods-use-this': 0,
    'no-extend-native': 0,
  }
}
